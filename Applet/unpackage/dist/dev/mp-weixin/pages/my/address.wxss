.main {
  position: relative;
}
.button {
  width: 80%;
  height: 40px;
  border-radius: 20px;
  position: fixed;
  bottom: 35px;
  left: 10%;
  text-align: center;
  line-height: 40px;
  font-size: 14px;
  background-color: #C7EEE6;
  color: #47c8ac;
  z-index: 10000;
}
.item {
  width: 100%;
  padding: 10px;
  box-sizing: border-box;
  margin-bottom: 15px;
  display: flex;
  position: relative;
  align-items: center;
}
.item .icon {
  width: 32px;
  height: 32px;
  border-radius: 100%;
  overflow: hidden;
  margin-right: 10px;
  background-color: rgba(71, 200, 172, 0.4);
  display: flex;
  font-size: 15px;
  color: #47C8AC;
  align-items: center;
  justify-content: center;
}
.item .icon image {
  width: 18px;
  height: 18px;
}
.item .content {
  width: calc(100% - 80px);
}
.item .content .me {
  display: flex;
  align-items: center;
}
.item .content .me text {
  font-size: 14px;
  color: #333;
  display: block;
  margin-right: 10px;
  font-weight: bold;
}
.item .content .me .isdef {
  border: 1px solid #47C8AC;
  padding: 2px 4px;
  font-size: 12px;
  color: #47C8AC;
  border-radius: 2px;
  font-weight: 300;
}
.item .content .address {
  display: block;
  width: 100%;
  font-size: 14px;
  color: #555555;
  margin-top: 6px;
}
.item .edit {
  width: 35px;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}

