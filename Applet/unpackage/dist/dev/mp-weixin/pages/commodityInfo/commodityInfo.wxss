page,
.main {
  width: 100%;
  height: 100%;
  background-color: #F5F6F8;
}
.bottom {
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 60px;
  background: #fff;
  border-top: 1px solid #eeeeee;
  display: flex;
  align-items: center;
  justify-content: space-around;
}
.bottom image {
  width: 25px;
  height: 25px;
}
.bottom view {
  width: calc((100% - 120px) / 2);
  border-radius: 20px;
  height: 40px;
  text-align: center;
  line-height: 40px;
  border: 1px solid #FF8200;
}
.bottom .cang {
  background-color: #fff;
  color: #FF8200;
}
.bottom .buy {
  background-color: #FF8200;
  color: #fff;
}
.com-box {
  width: 100%;
  height: 48px;
  display: flex;
  align-items: center;
  position: relative;
}
.com-box .title {
  font-size: 16px;
  color: #333;
}
.com-box .info {
  font-size: 14px;
  color: #aaa;
  display: block;
  margin-left: 10px;
}
.com-box .icon {
  height: 100%;
  width: 35px;
  display: flex;
  align-items: center;
  justify-content: center;
  position: absolute;
  right: 2px;
}
.select-item {
  width: 100%;
  display: flex;
  align-items: center;
  position: relative;
  height: 36px;
}
.select-item .lable {
  font-size: 15px;
  width: 45px;
  color: #aaa;
}
.select-item .text-spbox {
  width: calc(100% - 90px);
}
.select-item .icon {
  height: 100%;
  width: 35px;
  display: flex;
  align-items: center;
  justify-content: center;
  position: absolute;
  right: 2px;
}
.select_sp {
  width: 100%;
  margin-top: 10px;
  height: 1000rpx;
  padding: 10px;
  box-sizing: border-box;
  position: relative;
  padding-bottom: 60px;
}
.select_sp .title-address {
  display: block;
  width: 100%;
  text-align: center;
  font-size: 15px;
  color: #333;
  margin-bottom: 10px;
}
.select_sp .select_sp_top {
  display: flex;
  width: 100%;
}
.select_sp .select_sp_top image {
  width: 90px;
  height: 90px;
  display: block;
  margin-right: 10px;
}
.select_sp .select_sp_top .select_top_content {
  width: calc(100% - 100px);
}
.select_sp .select_sp_top .select_top_content .price {
  color: red;
  font-size: 22px;
  font-weight: bold;
  display: inline-block;
}
.select_sp .select_sp_top .select_top_content .price .do {
  font-size: 14px;
  display: inline-block;
}
.select_sp .select_sp_top .select_top_content .er_select {
  font-size: 15px;
  color: #333;
  margin-top: 5px;
  display: block;
  font-weight: 300;
}
.select_sp .select_sp_bottom {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  max-height: calc(100% - 190px);
  box-sizing: border-box;
  padding-top: 10px;
  overflow: auto;
}
.select_sp .select_sp_bottom .title {
  display: block;
  margin: 10px 0;
  width: 100%;
  font-size: 15px;
  color: #aaa;
}
.select_sp .select_sp_bottom .select_sp_item {
  padding: 4px 6px;
  font-size: 14px;
  border-radius: 4px;
  margin-right: 10px;
  max-width: 700rpx;
  margin-bottom: 15px;
}
.select_sp .select_sp_bottom .select_sp_ok {
  background-color: #C7EEE6;
  color: #47c8ac;
}
.select_sp .select_sp_bottom .select_sp_no {
  background-color: #F5F6F8;
  color: #333;
}
.select_sp .select_sp_num {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-top: 10px;
  width: 100%;
  padding-top: 10px;
  box-sizing: border-box;
  border-top: 1px solid #e1e1e1;
}
.select_sp .select_sp_num .title {
  display: block;
  margin: 10px 0;
  width: 100%;
  font-size: 15px;
  color: #aaa;
}
.select_sp .select_sp_model {
  width: 100%;
  height: 60px;
  display: flex;
  align-items: center;
  justify-content: center;
  position: absolute;
  bottom: 10px;
  box-sizing: border-box;
}
.select_sp .select_sp_model view {
  width: 320rpx;
  height: 40px;
  line-height: 40px;
  color: #fff;
  text-align: center;
}
.select_sp .select_sp_model .left {
  background-color: #FF821C;
  border-radius: 20px 0 0 20px;
}
.select_sp .select_sp_model .right {
  background-color: #F30300;
  border-radius: 0 20px 20px 0;
}
.select_sp .select_sp_model .add_address {
  width: 80%;
  background-color: #C7EEE6;
  color: #47c8ac;
  border-radius: 20px;
}
.select_sp .address_list {
  max-height: calc(100% - 50px);
  overflow: auto;
}
.select_sp .address_list .item {
  width: 100%;
  padding: 10px;
  box-sizing: border-box;
  margin-bottom: 15px;
  display: flex;
  position: relative;
  align-items: center;
}
.select_sp .address_list .item .icon {
  width: 32px;
  height: 32px;
  border-radius: 100%;
  overflow: hidden;
  margin-right: 10px;
  background-color: rgba(71, 200, 172, 0.4);
  display: flex;
  font-size: 15px;
  color: #47C8AC;
  align-items: center;
  justify-content: center;
}
.select_sp .address_list .item .icon image {
  width: 18px;
  height: 18px;
}
.select_sp .address_list .item .content {
  width: calc(100% - 80px);
}
.select_sp .address_list .item .content .me {
  display: flex;
  align-items: center;
}
.select_sp .address_list .item .content .me text {
  font-size: 14px;
  color: #333;
  display: block;
  margin-right: 10px;
  font-weight: bold;
}
.select_sp .address_list .item .content .me .isdef {
  border: 1px solid #47C8AC;
  padding: 2px 4px;
  font-size: 12px;
  color: #47C8AC;
  border-radius: 2px;
  font-weight: 300;
}
.select_sp .address_list .item .content .address {
  display: block;
  width: 100%;
  font-size: 14px;
  color: #555555;
  margin-top: 6px;
}
.select_sp .address_list .item .edit {
  width: 35px;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.kuai {
  display: block;
  width: 100%;
  padding-left: 45px;
  font-size: 13px;
  color: #aaa;
}
.e-info {
  background-color: #fff;
  border-radius: 5px;
  padding: 10px;
  width: calc(100% - 20px);
  margin-left: 10px;
  margin-top: 10px;
  box-sizing: border-box;
}
.e-info text {
  display: block;
}
.e-info .price {
  color: red;
  font-size: 22px;
  font-weight: bold;
  display: inline-block;
}
.e-info .price .do {
  font-size: 14px;
  display: inline-block;
}
.e-info .name {
  font-size: 15px;
  margin-top: 5px;
  line-height: 24px;
}
.e-info .brief {
  font-size: 13px;
  color: #aaa;
  margin-top: 5px;
}
.e-info .sales {
  font-size: 13px;
  color: #aaa;
  margin-top: 10px;
}
.list-box {
  width: 100%;
  padding: 10px;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  box-sizing: border-box;
}
.list-box .list-item {
  box-sizing: border-box;
  width: calc(50% - 5px);
  overflow: hidden;
  border-radius: 10px;
  background: #fff;
  box-shadow: rgba(99, 99, 99, 0.2) 0px 2px 8px 0px;
  margin-bottom: 22px;
  position: relative;
}
.list-box .list-item .tag {
  box-sizing: border-box;
  background-color: #46CBB3;
  color: #fff;
  font-size: 12px;
  border-radius: 2px;
  padding: 2px 4px;
  position: absolute;
  font-weight: 300;
  top: 5px;
  right: 5px;
}
.list-box .list-item .list-item-content {
  padding: 6px;
  height: 88px;
  position: relative;
}
.list-box .list-item .list-item-content .shop-name {
  font-size: 14px;
  line-height: 25px;
  color: #333;
  font-weight: 300;
}
.list-box .list-item .list-item-content .list-item-price {
  position: absolute;
  bottom: 10px;
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding-right: 22px;
  box-sizing: border-box;
}
.list-box .list-item .list-item-content .list-item-price .price {
  color: red;
  font-size: 18px;
}
.list-box .list-item .list-item-content .list-item-price .sales {
  font-size: 13px;
  color: #aaa;
}
.list-box .list-item image {
  width: 100%;
  height: 185px;
}

